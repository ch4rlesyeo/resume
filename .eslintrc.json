{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "security"
  ],
  "globals": {
    "fetch": false
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/prop-types": 0
  }
}
